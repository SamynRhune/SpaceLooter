@using ActionCommandGame.Model
@model PositiveGameEvent
@{
    ViewData["Title"] = $"Edit Positive Game Event {Model.Name}";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <partial name="PositiveEvents/CreateEditPartial" model="Model" />

    <button type="submit" class="btn btn-primary">Save</button>
</form>

@section Scripts
{
    <partial name="_ValidationScriptsPartial" />
}